<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	 ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	 ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"   
	 ui:generateLocales="default"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
    xmlns:b2="urn:import:com.github.gwtbootstrap.datepicker.client.ui"
    xmlns:c="com.bradrydzewski.gwt.calendar.client"
     >
    
    <ui:with field="res" type="com.binomed.sqli.gwt.client.resources.ProjectResources" />
	<ui:with field="i18n" type="com.binomed.sqli.gwt.client.resources.i18n.I18N" />
    
   <g:DockLayoutPanel unit="PC" height="500px" >
	   <g:center >
	   		<g:HorizontalPanel>
	   		<b2:DateBox format="dd/mm/yyyy" autoClose="false" ui:field="datePicker" />
	   		<b:TabPanel tabPosition="above" ui:field="tabPanel">
	   			<b:TabLink text="1 Day" dataTarget="calendarTab" createTabPane="false" ui:field="oneDay" />
	   			<b:TabLink text="3 Day" dataTarget="calendarTab" createTabPane="false"  ui:field="threeDay"/>
	   			<b:TabLink text="Work Week" dataTarget="calendarTab" createTabPane="false"  ui:field="workWeek" />
	   			<b:TabLink text="Week" dataTarget="calendarTab" createTabPane="false" active="true"  ui:field="week"/>
	   			<b:TabLink text="Month" dataTarget="calendarTab" createTabPane="false"  ui:field="month"/>
	   			<b:TabPane href="calendarTab" createTabLink="false">
   					<g:FlowPanel styleName="{res.css.contentCenter}" width="95%" height="500px" ui:field="calendarContent" />
	   			</b:TabPane>
	   		</b:TabPanel>
	   		</g:HorizontalPanel>
	   </g:center>
	   <g:east size="20" >
  			<b:Hero>
  				   <b:Heading size="2" text="Chat" />
  			</b:Hero>
	   </g:east>
   </g:DockLayoutPanel>
</ui:UiBinder>