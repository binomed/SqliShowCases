<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	 ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	 ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"   
	 ui:generateLocales="default"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
    xmlns:b2="urn:import:com.github.gwtbootstrap.datepicker.client.ui"
    xmlns:c="com.bradrydzewski.gwt.calendar.client"
     >
    
    <ui:with field="res" type="com.binomed.sqli.gwt.client.resources.ProjectResources" />
	<ui:with field="i18n" type="com.binomed.sqli.gwt.client.resources.i18n.I18N" />
    
   <g:DockLayoutPanel unit="PC" height="500px" >
   	   <g:north size="5">
   	   	<g:HorizontalPanel>
   	   	 	<b:Heading size="3" text="{i18n.eventTitle}" />
   	   	 	<b:Heading size="3" text="{i18n.eventBackEvents}" />
   	   	</g:HorizontalPanel>
   	   </g:north>
	   <g:center >
	   		<g:VerticalPanel>
   	   		<b:Heading size="2" ui:field="eventName" text="Un text"/>
   	   		
	   		
	   		</g:VerticalPanel>
	   </g:center>
	   <g:east size="45" >
  			<b:Hero>
  				   <b:Heading size="2" text="Chat" />
  			</b:Hero>
	   </g:east>
   </g:DockLayoutPanel>
</ui:UiBinder>