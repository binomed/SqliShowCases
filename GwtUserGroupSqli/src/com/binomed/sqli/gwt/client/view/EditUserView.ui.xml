<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
    xmlns:sqli="urn:import:com.binomed.sqli.gwt.client.editor"
     >
    
    <ui:with field="res" type="com.binomed.sqli.gwt.client.resources.ProjectResources" />
	<ui:with field="i18n" type="com.binomed.sqli.gwt.client.resources.i18n.I18N" />
    <g:HTMLPanel>
    	<b:FluidRow>
    		<b:Column size="6" offset="4">
   				<b:Hero>
					<g:FlowPanel ui:field="userEditor" styleName="{res.css.contentCenter}"/>
				</b:Hero>
    		</b:Column>
    	</b:FluidRow>
    </g:HTMLPanel>
</ui:UiBinder>